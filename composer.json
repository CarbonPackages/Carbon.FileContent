{
    "name": "carbon/filecontent",
    "type": "neos-plugin",
    "description": "Include file content helper for Neos CMS",
    "license": "MIT",
    "require": {
        "neos/neos": "~3.0 || dev-master"
    },
    "autoload": {
        "psr-4": {
            "Carbon\\FileContent\\": "Classes"
        }
    },
    "archive": {
        "exclude": [".editorconfig", ".gitignore", ".github", ".gitattributes", "CODE_OF_CONDUCT.md"]
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://github.com/jonnitto",
            "role": "Developer"
        }
    ],
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Carbon.FileContent",
        "neos": {
            "package-key": "Carbon.FileContent"
        }
    }
}
